{
    "name": "Get Title",

    "version": "1.0",

    "manifest_version": 3,

    "description": "Fetches the title of the current tab and displays it in the popup window.",

    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    
    "icons" :{
        "16": "/images/picker_16.png",
        "32": "/images/picker_32.png",
        "48": "/images/picker_48.png"
    }
  }
  